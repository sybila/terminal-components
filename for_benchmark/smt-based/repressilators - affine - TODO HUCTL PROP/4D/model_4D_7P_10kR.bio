VARS: x,y,z,v
CONSTS: k4,1
PARAMS: k1,0,10; k2,0,10; k3,0,10; deg_x,0,1; deg_y,0,1; deg_z,0,1; deg_v,0,1

EQ: x = k1*hillm(y,5,5,1,0) - deg_x*x
EQ: y = k2*hillm(z,5,5,1,0) - deg_y*y
EQ: z = k3*hillm(v,5,5,1,0) - deg_z*z
EQ: v = k4*hillm(x,5,5,1,0) - deg_v*v

VAR_POINTS: x: 2500, 10; y: 2500, 10; z: 2500, 10; v: 2500, 10

THRES: x: 0, 15
THRES: y: 0, 15
THRES: z: 0, 15
THRES: v: 0, 15
